{
  "name": "directus-extension-dummy",
  "description": "Please enter a description for your extension",
  "version": "1.0.0",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-bundle"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "directus:extension": {
    "host": "^10.1.10",
    "type": "bundle",
    "path": {
      "app": "dist/app.js",
      "api": "dist/api.js"
    },
    "entries": [
      {
        "type": "module",
        "name": "module",
        "source": "src/module/index.ts"
      },
      {
        "type": "endpoint",
        "name": "endpoint",
        "source": "src/endpoint/index.ts"
      },
      {
        "type": "hook",
        "name": "hook",
        "source": "src/hook/index.ts"
      }
    ]
  },
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link",
    "add": "directus-extension add"
  },
  "devDependencies": {
    "@directus/errors": "^0.2.4",
    "@directus/extensions-sdk": "11.0.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "express": "^4.19.1",
    "typescript": "^5.4.3",
    "vue": "^3.4.21"
  }
}
